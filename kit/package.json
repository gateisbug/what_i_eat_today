{
  "name": "kit",
  "version": "1.0.0",
  "keywords": [
    "react",
    "typescript"
  ],
  "scripts": {
    "build": "rm -rf ./dist && rollup -c --bundleConfigAsCjs",
    "watch": "rollup -c -w --bundleConfigAsCjs"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Badge": {
      "import": "./dist/Badge/index.js",
      "types": "./dist/Badge/index.d.ts"
    },
    "./Button": {
      "import": "./dist/Button/index.js",
      "types": "./dist/Button/index.d.ts"
    },
    "./Check": {
      "import": "./dist/Check/index.js",
      "types": "./dist/Check/index.d.ts"
    },
    "./Chip": {
      "import": "./dist/Chip/index.js",
      "types": "./dist/Chip/index.d.ts"
    },
    "./Input": {
      "import": "./dist/Input/index.js",
      "types": "./dist/Input/index.d.ts"
    },
    "./Modal": {
      "import": "./dist/Modal/index.js",
      "types": "./dist/Modal/index.d.ts"
    },
    "./Progress": {
      "import": "./dist/Progress/index.js",
      "types": "./dist/Progress/index.d.ts"
    },
    "./Radio": {
      "import": "./dist/Radio/index.js",
      "types": "./dist/Radio/index.d.ts"
    },
    "./ScrollView": {
      "import": "./dist/ScrollView/index.js",
      "types": "./dist/ScrollView/index.d.ts"
    },
    "./Select": {
      "import": "./dist/Select/index.js",
      "types": "./dist/Select/index.d.ts"
    },
    "./Skeleton": {
      "import": "./dist/Skeleton/index.js",
      "types": "./dist/Skeleton/index.d.ts"
    },
    "./Textarea": {
      "import": "./dist/Textarea/index.js",
      "types": "./dist/Textarea/index.d.ts"
    }
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "partially supports css-grid"
  ],
  "packageManager": "yarn@4.1.1",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@types/styled-components": "^5.1.34",
    "autoprefixer": "^10.4.16",
    "babel-plugin-styled-components": "^2.1.4",
    "cssnano": "^6.0.3",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-preset-env": "^9.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.3.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-swc3": "^0.10.3",
    "styled-components": "^6.1.8",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4"
  },
  "license": "MIT"
}
